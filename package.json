{
  "name": "idcard",
  "version": "0.1.0",
  "description": "UW Web Services Registration POC",
  "main": "dist",
  "scripts": {
    "dev": "set NODE_ENV=dev&& nodemon -w src --exec \"babel-node src --presets es2015,stage-0,react\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0,react&& webpack",
    "start": "node dist",
    "prestart": "set NODE_ENV=prod&& npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwwebservices/idcard-webapp-poc.git"
  },
  "author": "UWWebServices <ews-team@uw.edu>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "cheerio": "1.0.0-rc.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "nodemon": "^1.9.2",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.18",
    "uglifyjs-webpack-plugin": "^0.4.4",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
