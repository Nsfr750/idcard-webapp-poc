{
  "name": "idcard",
  "version": "0.1.0",
  "description": "UW Web Services Registration POC",
  "main": "dist",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-2,react --plugins transform-object-rest-spread && webpack",
    "start": "cross-env NODE_ENV=prod node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwwebservices/idcard-webapp-poc.git"
  },
  "author": "UWWebServices <ews-team@uw.edu>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cheerio": "1.0.0-rc.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "morgan": "^1.8.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.18",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
